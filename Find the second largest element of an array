def getSecondLargest(arr):
    largest=arr[0]
    slargest=-1
    sorted_element=arr.sort()
    for i in range(len(arr)):
        if arr[i]>largest:
            slargest=largest
            largest=arr[i]
        elif arr[i]<largest and arr[i]>slargest:
            slargest=arr[i]
        if len(arr)<2:
            return -1
    return slargest
# getSecondLargest([12,35,1,10,34,1])

getSecondLargest([10,10,25])
